#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int T;
	cin>>T;
	
	while(T--)
	{
	    
	   string bin;
	   cin>>bin;
	   
	   long int result = 0;
	   
	   for(int i=bin.length()-1;i>=0;i--)
	    {
             if(bin[i]=='1')
	        result += pow(2,bin.length()-i-1);
	        
	    }
	   
	   cout<<result;
	   cout<<endl;
	    
	    
	    
	}
	return 0;
}
